    "react-dom": "^16.13.1",
    "react-media": "^1.10.0",
    "react-modal": "^3.11.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "sass-loader": "^8.0.2",
