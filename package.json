    "ua-parser-js": "^0.7.21",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "uniqid": "^5.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.18.1",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@pinua/presets": "*"
  },
  "browserslist": {
    "production": [
      ">0.2%"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "workspaces": [
    "packages/*",
    "server"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix packages src -c ./.eslintrc.js",
      "git add"
    ],
    "*.scss": [
      "stylelint --config ./.stylelintrc --cache './src/**/**/*.css'",
      "git add"
    ]
  }
}
