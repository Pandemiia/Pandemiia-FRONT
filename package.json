{
  "name": "pandemiainua",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "start": "cross-env NODE_ENV=production && node ./server/bin/www.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack --progress -p",
    "deploy:packages": "lerna publish",
    "lint": "run-p lint:js lint:scss",
    "lint:js": "eslint --fix packages src -c ./.eslintrc.js",
    "lint:scss": "stylelint --fix --config ./.stylelintrc --cache './src/**/**/*.scss'",
    "storybook": "start-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s public",
    "test": "jest --coverage -u -c=./webpack/jest.config.js"
  },
  "dependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-options": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "cache-loader": "^4.1.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^4.5.3",
    "cross-env": "^5.2.1",
    "css-loader": "^0.23.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
